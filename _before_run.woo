-- 启动http服务时运行一次，用于配置程序


package.path = '@' .. _DIR .. '/vendor/?.woo'

print('before run:===', package.path)

local env = require('oshine/woo_mvc@1.?:env')

local addr, psw, db, db_conn = env('redis_addr', 'redis_psw', 'redis_db', 'db_conn')

require('oshine/woo_mvc@1?:redis')
        .conf({ addr = addr, psw = psw, db = db })

require('oshine/woo_mvc@1?:db')
        .conf({ db_conn = db_conn })


