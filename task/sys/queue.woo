return {
    start = function(p,lan)
        local queue = require('oshine/woo_mvc@1?:queue')
        queue.start()
    end,
    stop = function(p,lan)
        local queue = require('oshine/woo_mvc@1?:queue')
        queue.stop()
    end,
    reload = function(p,lan)
        local queue = require('oshine/woo_mvc@1?:queue')
        queue.reload()
    end
}